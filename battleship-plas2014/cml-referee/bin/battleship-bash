#!/bin/bash

# start cml-referee version battleship

# set sml to SML/NJ executable

sml=/usr/local/bin/sml

# edit the following line to set dist to the full pathname of
# the cml-referee directory

dist=/usr/local/share/battleship/battleship-plas2014/cml-referee

exec $sml @SMLload=$dist/bin/battleship-heap-image @SMLcmdname=$0 "$@"
